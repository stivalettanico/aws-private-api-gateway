{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Deny",
        "Principal": "*",
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:${region}:${account_id}:${api_id}/*/*/*",
        "Condition": {
          "StringNotEquals": {
            "aws:sourceVpce": "${vpce_id}"
          }
        }
      },
      {
        "Effect": "Allow",
        "Principal": "*",
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:${region}:${account_id}:${api_id}/*/*/*"
      }
    ]
  }
  